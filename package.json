{
  "name": "pixel-face",
  "version": "1.0.4",
  "description": "A pixel face generator",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json --passWithNoTests",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CGReinhold/pixel-face.git"
  },
  "keywords": [
    "pixel",
    "face"
  ],
  "author": "Cleyson Reinhold",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CGReinhold/pixel-face/issues"
  },
  "homepage": "https://github.com/CGReinhold/pixel-face",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.3"
  },
  "files": [
    "lib/**/*"
  ]
}
